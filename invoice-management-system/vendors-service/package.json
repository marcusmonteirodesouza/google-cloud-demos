{
  "name": "google-cloud-demos-invoice-management-system-vendors-service",
  "version": "1.0.0",
  "description": "Google Cloud Demos - Invoice Management System - Vendors Service",
  "private": true,
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean && npm run clean:pg",
    "clean:pg": "rm -rf postgresql-data",
    "compile": "tsc",
    "fix": "npx prettier --write . && gts fix",
    "start": "ts-node-dev -r dotenv/config src/index.ts",
    "up:pg": "docker compose up pg --detach --wait",
    "down": "docker compose down",
    "knex": "knex",
    "migrate:make": "knex migrate:make -x js",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "prestart": "npm run up:pg"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.10",
    "@types/express": "^4.17.20",
    "@types/node": "20.8.2",
    "@types/pg": "^8.10.7",
    "gts": "^5.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.2.0"
  },
  "dependencies": {
    "@google-cloud/logging-bunyan": "^5.0.1",
    "bunyan": "^1.8.15",
    "celebrate": "^15.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "knex": "^3.0.1",
    "knex-stringcase": "^1.4.6",
    "pg": "^8.11.3"
  }
}
