{
  "name": "google-cloud-demos-invoice-management-system-invoices-management-app",
  "version": "1.0.0",
  "description": "Google Cloud Demos - Invoice Management System - Invoices Management App",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc && cp -R ./src/public ./build/src/ && cp -R src/views build/src/",
    "fix": "npx prettier --write . && gts fix",
    "dev": "ts-node-dev -r dotenv/config src/index.ts",
    "start": "node -r dotenv/config build/src/index.js",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "prestart": "npm run clean && npm run compile"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.201",
    "@types/node": "20.8.2",
    "dotenv": "^16.3.1",
    "gts": "^5.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.2.0"
  },
  "dependencies": {
    "@google-cloud/compute": "^4.0.1",
    "@google-cloud/logging-bunyan": "^5.0.1",
    "axios": "^1.6.0",
    "bunyan": "^1.8.15",
    "express": "^4.18.2",
    "google-auth-library": "^9.2.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "pug": "^3.0.2"
  }
